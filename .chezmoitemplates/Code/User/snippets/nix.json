{
  "nixpkgs": {
    "prefix": "nixpkgs",
    "body": "import (fetchTarball \"https://github.com/NixOS/nixpkgs/archive/${1:nixpkgs-unstable}.tar.gz\") {}",
    "description": "Import the nixpkgs tarball",
  },
  "flake": {
    "prefix": "flake",
    "body": [
      "{",
      "\tdescription = \"${1:Lorem ipsum}\";",
      "",
      "\tinputs = {",
      "\t\tnixpkgs.url = \"nixpkgs\";",
      "\t\tflake-utils.url = \"flake-utils\";",
      "\t};",
      "",
      "\toutputs = { self, nixpkgs, flake-utils, ... }@inputs:",
      "\tlet",
      "\t\tsupportedSystems = [",
      "\t\t\tflake-utils.lib.system.x86_64-linux",
      "\t\t\tflake-utils.lib.system.aarch64-linux",
      "\t\t\tflake-utils.lib.system.x86_64-darwin",
      "\t\t\tflake-utils.lib.system.aarch64-darwin",
      "\t\t];",
      "\tin",
      "\t\tflake-utils.lib.eachSystem supportedSystems (system:",
      "\t\tlet",
      "\t\t\tpkgs = import nixpkgs { inherit system; };",
      "\t\tin",
      "\t\t{",
      "\t\t\tpackages.default = ${0:pkgs.hello};",
      "",
      "\t\t\tapps.default = {",
      "\t\t\t\ttype = \"app\";",
      "\t\t\t\tprogram = \"\\${self.packages.\\${system}.default}/bin/hello\";",
      "\t\t\t};",
      "",
      "\t\t\tdevShells.default = pkgs.mkShell {",
      "\t\t\t\tpackages = [];",
      "\t\t\t};",
      "\t\t}) // {",
      "\t\t\tlib = {};",
      "\t\t\toverlays = {};",
      "\t\t\tnixosModules = {};",
      "\t\t\tnixosConfigurations = {};",
      "\t\t};",
      "}"
    ],
    "description": "Nix Flake",
    "isFileTemplate": true
  }
}
